{
  "builders": [
    {
      "type": "openbsd-vmm",
      "vm_name": "openbsd",
      "vm_template": "generic",
      "disk_format": "qcow2",
      "disk_size": "20G",
      "boot": "/var/www/htdocs/openbsd/snapshots/amd64/bsd.rd",
      "log_directory": "/home/packer_user/.log/packer",
      "output_directory": "/home/packer_user/.local/share/packer",
      "boot_wait": "15s",
      "boot_command": [
	"http://{{ .HTTPIP }}:{{ .HTTPPort }}/packer-auto_install-iso.conf<enter>",
	"I<enter>"
      ],
      "ssh_username": "root"
    }
  ]
}
