{
  "builders": [
    {
      "type": "openbsd-vmm",
      "vm_name": "openbsd",
      "vm_template": "generic",
      "disk_format": "qcow2",
      "disk_size": "20G",
      "boot": "/var/www/htdocs/openbsd/snapshots/amd64/bsd.rd",
      "log_directory": "/home/packer_user/.log/packer",
      "output_directory": "/home/packer_user/.local/share/packer",
      "boot_wait": "15s",
      "boot_command": [
	"http://192.168.255.1/autoinstall/packer-install.conf<enter>"
      ],
      "ssh_username": "root"
    }
  ]
}
